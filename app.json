{
  "expo": {
    "name": "Skiftappen",
    "slug": "skiftappen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skiftappen.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Denna app behöver tillgång till kameran för att ta profilbilder.",
        "NSPhotoLibraryUsageDescription": "Denna app behöver tillgång till fotobiblioteket för att välja profilbilder."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.skiftappen.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Appen behöver tillgång till dina foton för att välja profilbild.",
          "cameraPermission": "Appen behöver tillgång till kameran för att ta profilbilder."
        }
      ]
    ],
    "scheme": "skiftappen",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}